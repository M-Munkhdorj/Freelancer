<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="1000.0" prefWidth="1600.0" stylesheets="@css/main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.freelancer.Login.SignUpController">
   <children>
      <TextField fx:id="tfUsername" layoutX="650.0" layoutY="414.0" prefHeight="50.0" prefWidth="300.0" promptText="John Doe" />
      <Text fill="#667080" layoutX="650.0" layoutY="400.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="textEmail" text="Username" />
      <Button fx:id="btnContinue" layoutX="650.0" layoutY="564.0" mnemonicParsing="false" onMousePressed="#onContinueButtonPressed" onMouseReleased="#onContinueButtonReleased" prefHeight="50.0" prefWidth="300.0" styleClass="buttonContinue" text="Continue" />
      <TextField fx:id="tfPassword" layoutX="650.0" layoutY="497.0" prefHeight="50.0" prefWidth="300.0" />
      <Text fill="#667080" layoutX="650.0" layoutY="486.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="textEmail" text="Password" />
      <Text fill="#667080" layoutX="650.0" layoutY="633.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="textNotUser" text="Already a user?" />
      <Button fx:id="btnSignIn" layoutX="650.0" layoutY="643.0" mnemonicParsing="false" onMousePressed="#onSignInButtonPressed" onMouseReleased="#onSignInButtonReleased" prefHeight="50.0" prefWidth="200.0" styleClass="buttonCreateAccount" text="Sign in" />
      <TextField fx:id="tfEmail" layoutX="650.0" layoutY="331.0" prefHeight="50.0" prefWidth="300.0" promptText="example@gmail.com" />
      <Text fill="#667080" layoutX="650.0" layoutY="317.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="textEmail" text="Email" />
   </children>
</AnchorPane>
